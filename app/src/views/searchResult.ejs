<!-- views/searchResult.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>검색 결과</title>
</head>
<body>
    <h1>검색 결과</h1>

    <% if (searchResults.length > 0) { %>
        <p><strong>Username:</strong> <%= searchResults[0].username %></p>
        <p><strong>MBTI:</strong> <%= searchResults[0].mbti %></p>
        <p><strong>Introduction:</strong> <%= searchResults[0].introduction %></p>
    <% } else { %>
        <p>검색 결과가 없습니다.</p>
    <% } %>

    <a href="/profile/searchUser">다시 검색하기</a>
</body>
</html>
